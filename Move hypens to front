#include <stdio.h>
#include <string.h>
int main()
{
   char str[30];
   scanf("%s",str);
   char hy[10];
   int len = strlen(str);
   int i,j=0;
   for(int i=0;i<len;i++)
   {
       if(str[i] == '-')
       {
           hy[j] = str[i];
           j++;
       }
   }
   for(int i=0;i<j;i++)
   {
       printf("%c",hy[i]);
   }
   for(int i=0;i<len;i++)
   {
       if(str[i] != '-')
       {
           printf("%c",str[i]);
       }
   }
   return 0;
}
