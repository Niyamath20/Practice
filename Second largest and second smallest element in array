#include <stdio.h>
int main()
{
    int n,temp = 0;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<n-1;i++)
    {
        for(int j=0;j<n-i-1;j++)
        {
            if(arr[j] > arr[j+1])
            {
                temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
    }
    
    for(int i=0;i<n-1;i++)
    {
        if(arr[i] == arr[i+1])
        {
            continue;
        }
        else
        {
            printf("2nd smallest element is %d\n",arr[i+1]);
            break;
        }
    }
    for(int i=n-1;i>0;i--)
    {
        if(arr[i] == arr[i-1])
        {
            continue;
        }
        else
        {
            printf("2nd largest element is %d",arr[i-1]);
            break;
        }
    }
    return 0;
}
