/******************************************************************************

IP : 7
r g b b g y y 

r - 1
g - 2
b - 2
y - 2

print odd numbers in OP
OP : r

*******************************************************************************/
#include <stdio.h>
#include <string.h>
int main()
{
    int n;
    scanf("%d",&n);
    char ch[n];
    scanf("%s",ch);
    int len = strlen(ch);
    if(n != len)
    {
        printf("Invalid Input!!");
        return 0;
    }
    int c = 0;
    for(int i=0;i<n;i++)
    {
        c = 0;
        for(int j=0;j<n;j++)
        {
            if(ch[i] == ch[j])
            {
                c++;
            }
        }
        if(c % 2 != 0)
        {
            printf("%c",ch[i]);
        }
    }

    return 0;
}

