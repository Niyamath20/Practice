#include <stdio.h>

int main()
{
    int sum;
    printf("Enter sum : ");
    scanf("%d",&sum);
    int size;
    printf("Enter array size : ");
    scanf("%d",&size);
    int arr[size];
    for(int i=0;i<size;i++)
    scanf("%d",&arr[i]);
    for(int i=0;i<size-1;i++)
    {
        for(int j=0;j<size-i-1;j++)
        {
            if(arr[j] > arr[j+1])
            {
            int temp = arr[j];
            arr[j] = arr[j+1];
            arr[j+1] = temp;
            }
        }
    }
    int a=0,pr=0;
    for(int i=0;i<size-1;i++)
    {
        if(arr[i] != arr[i+1])
        {
            a = arr[i] + arr[i+1];
            pr = arr[i] * arr[i+1];
            break;
        }
    }
    if(a < sum )
    {
        printf("%d",pr);
        return 0;
    }
    else if(a >= sum)
    {
        printf("0");
        return 0;
    }
    else if (size == 0)
    {
        printf("-1");
        return 0;
    }
}
